class Solution:
    def getLeastNumbers(self, arr: List[int], k: int) -> List[int]:
        """最小堆"""
        if k == 0:
            return list()
        hp = [-x for x in arr[:k]]
        heapq.heapify(hp)
        for i in range(k,len(arr)):
            if -hp[0] > arr[i]:
                heapq.heappop(hp)
                heapq.heappush(hp,-arr[i])
        rs = [-x for x in hp]
        return rs
